   "execution_count": 14,
   "id": "37bfa0eb-32de-44e4-9c79-8181c0b6eb10",
   "metadata": {},
   "outputs": [],
   "source": [
    "import requests\n",
    "\n",
    "# URLs of the files\n",
    "csv_url = \"https://cf-courses-data.s3.us.cloud-object-storage.appdomain.cloud/IBM-DS0321EN-SkillsNetwork/datasets/spacex_launch_dash.csv\"\n",
    "py_url = \"https://cf-courses-data.s3.us.cloud-object-storage.appdomain.cloud/IBM-DS0321EN-SkillsNetwork/labs/module_3/spacex_dash_app.py\"\n",
    "\n",
    "# Download the CSV file\n",
    "csv_response = requests.get(csv_url)\n",
    "with open('spacex_launch_dash.csv', 'wb') as file:\n",
    "    file.write(csv_response.content)\n",
    "\n",
    "# Download the Python script\n",
    "py_response = requests.get(py_url)\n",
    "with open('spacex_dash_app.py', 'wb') as file:\n",
    "    file.write(py_response.content)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "8cc10249-342a-4243-91b6-af0f8ce7cac7",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/html": [
       "\n",
       "        <iframe\n",
       "            width=\"100%\"\n",
       "            height=\"650\"\n",
       "            src=\"http://127.0.0.1:8050/\"\n",
       "            frameborder=\"0\"\n",
       "            allowfullscreen\n",
       "            \n",
       "        ></iframe>\n",
